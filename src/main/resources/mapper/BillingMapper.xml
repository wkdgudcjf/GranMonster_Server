<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ronaldo.mapper.BillingMapper">

    <select id="getUserList" resultType="BillingVo">
        SELECT * FROM Billing
    </select>
	<insert id="registBilling" parameterType="BillingVo">
      	INSERT INTO Billing (userID,userEmail,userKey,userPassword,userCoin,userMoney) 
      	VALUES (#{userID},#{userEmail},#{userKey},#{userPassword},#{userCoin},#{userMoney})
    </insert>
</mapper>